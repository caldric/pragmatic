<!DOCTYPE html>
<html lang="en">
<%- include('../partials/head.ejs') %>
<body>
  <header>
    <nav>
      <a href="#">Pragmatic</a>
      <ul>
        <li><a href="#">Link 1</a></li>
        <li><a href="#">Link 2</a></li>
      </ul>
    </nav>
    <h1>Backlog</h1>
  </header>
  <main>
    <h2>Backlog</h2>
    <ul>
      <% issues.forEach(issue => { %>
        <li><a href="<%= baseUrl %>/issues/<%= issue.id %>"><%= issue.title %></a></li>
      <% }) %>
    </ul>
    <a href="<%= baseUrl %>/issues/new">+ Create issue</a>
  </main>
</body>
</html>
